        "adaptivecards": "^2.8.0",
        "adaptivecards-templating": "^2.1.0",
        "botbuilder": "~4.12.0",
        "botframework-schema": "^4.12.0",
        "restify": "^8.5.1"
    },
    "devDependencies": {
        "@types/restify": "^8.5.1",
        "clean-webpack-plugin": "^3.0.0",
        "concurrently": "^5.3.0",
        "html-webpack-plugin": "^4.3.0",
        "tinylicious": "0.4.17169",
        "ts-loader": "^8.0.17",
        "typescript": "~4.1.3",
        "webpack": "^4.46.0",
        "webpack-cli": "^4.2.0",
